@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<h1>Example Table Data</h1>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var row in Model.TableData)
    {
        <tr>
            <td>@row.id</td>
            <td>@row.name</td>
        </tr>
    }
    </tbody>
</table>
